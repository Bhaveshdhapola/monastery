{
    "timeout": 120000,
    "enableGZIPEncoding": true,
    "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, GET",
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Expose-Headers": "*",
        "Access-Control-Max-Age": 86400
    },
    "host": "monastery.codes",
    "port": 9092,
    "ssl": true,
    "forceHTTP1": true,
    "sslKeyFile":"/etc/letsencrypt/live/monastery.codes/privkey.pem",
    "sslCertFile":"/etc/letsencrypt/live/monastery.codes/fullchain.pem",
    "ipblacklistRefresh": 5000,
    "ipwhitelistRefresh": 5000
}
